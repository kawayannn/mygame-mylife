.login
  %h2.h2 Log in
  = form_for(resource, as: resource_name, url: session_path(resource_name)) do |f|
    .field
      = f.label :name
      %br/
      = f.text_field :name,autofocus: true, autocomplete: "name",value: "aaa"
    .field
      = f.label :email
      %br/
      = f.email_field :email, autofocus: true, autocomplete: "email",value: "aa@aa"
    .field
      = f.label :password
      %br/
      = f.password_field :password, autocomplete: "current-password", value: "aaaaaa"
    - if devise_mapping.rememberable?
      .field
        = f.check_box :remember_me
        = f.label :remember_me
    .actions
      = f.submit "Log in"
  = render "users/shared/links"
